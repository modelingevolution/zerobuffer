      "Bash(# Extract step patterns from EdgeCasesSteps.cs\ngrep -E ''^\\s*\\[(Given|When|Then)\\(@\"\"'' ZeroBuffer.Tests/StepDefinitions/EdgeCasesSteps.cs | sed ''s/.*@\"\"//'' | sed ''s/\"\".*//'' > /tmp/step_definitions.txt\n\n# Check each pattern against the feature file\necho \"\"=== UNUSED STEP DEFINITIONS ===\"\"\nwhile IFS= read -r pattern; do\n    # Convert regex pattern to a searchable form\n    search_pattern=$(echo \"\"$pattern\"\" | sed ''s/\\\\(/(/g'' | sed ''s/\\\\)/)/g'' | sed \"\"s/\\\\\\''/''/g\"\")\n    if ! grep -qF \"\"$search_pattern\"\" ZeroBuffer.Tests/Features/EdgeCases.feature; then\n        echo \"\"UNUSED: $pattern\"\"\n    fi\ndone < /tmp/step_definitions.txt)",
      "Bash(# Get unique steps from feature file\ngrep -E \"\"^\\s*(Given|When|Then|And)\"\" ZeroBuffer.Tests/Features/EdgeCases.feature | sed ''s/^\\s*//'' | sed ''s/^And //'' | sort -u > /tmp/feature_steps.txt\n\necho \"\"=== MISSING STEP DEFINITIONS ===\"\"\nwhile IFS= read -r step; do\n    # Remove the Given/When/Then prefix and get just the text\n    step_text=$(echo \"\"$step\"\" | sed ''s/^[^[:space:]]* //'')\n    \n    # Check if this step pattern exists in EdgeCasesSteps.cs\n    if ! grep -q \"\"$step_text\"\" ZeroBuffer.Tests/StepDefinitions/EdgeCasesSteps.cs; then\n        echo \"\"MISSING: $step\"\"\n    fi\ndone < /tmp/feature_steps.txt)",
